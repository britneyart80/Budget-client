
{{#each envelopes as |envelope|}}
  {{log envelope}}
  <section class="envelope" data-id="{{envelope.id}}">
    <h6>
      {{convert envelope.month}} ,
      {{#if envelope.year}}
      {{envelope.year}}
      {{else}}
      No year provided
      {{/if}}
    </h6>
  </br>
  <button class="btn btn-danger delete-envelope" data-id="{{id}}">Delete Envelope</button>
  </section>

{{/each}}
